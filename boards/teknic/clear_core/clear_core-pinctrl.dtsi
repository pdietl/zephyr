	sercom7_uart_default: sercom7_uart_default {
		group1 {
			pinmux = <PB20D_SERCOM7_PAD1>,
				 <PB21D_SERCOM7_PAD0>;
		};
	};

	sercom6_spi_default: sercom6_spi_default {
		group1 {
			pinmux = <PC5C_SERCOM6_PAD1>,
				 <PC6C_SERCOM6_PAD2>,
				 <PC7C_SERCOM6_PAD3>;
		};
	};

	sercom0_spi_default: sercom0_spi_default {
		group1 {
			pinmux = <PA8C_SERCOM0_PAD0>,
				 <PA9C_SERCOM0_PAD1>,
				 <PA11C_SERCOM0_PAD3>;
		};
	};
